@{
    ViewData["Title"] = "Your business is not eligible due to the company size";
}

    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-l">Your business is not eligible</h1>
            <p class="govuk-body">
                This is because the business does not employ between 1 and 249 people.
            </p>
            <h2 class="govuk-heading-m">Contact us</h2>
            <p class="govuk-body">
                If you would like to enquire about this decision, you can contact the Help to Grow: Digital team at: <a href="mailto:sme.helptogrow@beis.gov.uk" class="govuk-link">sme.helptogrow@beis.gov.uk</a>
            </p>
            <p>For further guidance on Help to Grow: Digital please visit: <a href="/guidance" class="govuk-link">General guidance</a>.</p>
        </div>
    </div>
